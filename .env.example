# Environment Variables for Ecommerce App
# Copy this file to .env for local development

# App Configuration
VITE_APP_NAME="IndiaKart - India's Premier Online Marketplace"
VITE_APP_VERSION="1.0.0"

# API Configuration (for future backend integration)
# VITE_API_BASE_URL=https://your-api-domain.com/api
# VITE_API_TIMEOUT=10000

# Payment Gateway (for future integration)
# VITE_RAZORPAY_KEY=your_razorpay_key
# VITE_PAYTM_MID=your_paytm_merchant_id

# Analytics (for future integration)
# VITE_GA_TRACKING_ID=your_google_analytics_id
# VITE_HOTJAR_ID=your_hotjar_id

# Feature Flags
VITE_ENABLE_DARK_MODE=true
VITE_ENABLE_NOTIFICATIONS=true